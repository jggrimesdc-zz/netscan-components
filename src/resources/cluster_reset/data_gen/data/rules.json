{
  "rules": [
    [
      "\\bids?\\b",
      "uuid4"
    ],
    [
      "\\bemail\\b",
      "email"
    ],
    [
      "^is\\b",
      "pybool"
    ],
    [
      "^has\\b",
      "pybool"
    ],
    [
      "\\bports?\\b",
      "port_number",
      {
        "is_system": false
      }
    ],
    [
      "\\bip\\b",
      "ipv4"
    ],
    [
      "\\b(company|companies|organizations?)_names?\\b",
      "company"
    ],
    [
      "\\bdomains?\\b",
      "domain_name"
    ],
    [
      "\\bstatus(es)?$",
      "random_element",
      {
        "elements": [
          "success",
          "failure",
          "pending"
        ]
      }
    ],
    [
      "\\repos?\\b",
      "domain_name"
    ],
    [
      "\\bs3\\w*urls?\\b",
      "url",
      {
        "schemes": [
          "s3"
        ]
      }
    ],
    [
      "\\burls?\\b",
      "url"
    ],
    [
      "\\bcountr(y|ies)\\b",
      "country"
    ],
    [
      "\\bmd5\\b",
      "md5"
    ],
    [
      "\\bsha256\\b",
      "sha256"
    ],
    [
      "\\bservers?\\b",
      "ipv4"
    ],
    [
      "\\b(teams?|personnel)\\b",
      "job"
    ],
    [
      "\\b(systems?|services?)\\b",
      "catch_phrase"
    ],
    [
      "\\b(info|information|desc|description)\\b",
      "sentence"
    ],
    [
      "\\b(docs?|documents?|documentation)\\b",
      "file_path"
    ],
    [
      "\\b(locs?|locations?)\\b",
      "country_code"
    ],
    [
      "\\b(metadata)\\b",
      "file_path"
    ],
    [
      "\\b(present|trusted)$",
      "pybool"
    ]
  ]
}
